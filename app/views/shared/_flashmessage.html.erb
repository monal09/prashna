<% #FIXME_AB: all all other types of flash messages -%>
<% if flash[:alert] %>
  <div class="alert alert-dismissible alert-warning">
    <p><%= flash[:alert] %></p>
  </div>
<% end %>

<% if flash[:errors] %>
  <div class="alert alert-dismissible alert-danger">
    <p><%= flash[:errors] %></p>
  </div>
<% end %>

