<h1>Users List</h1>
<table class="table table-striped table-hover ">
  <thead>
    <tr>
      <th>#</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Admin</th>
      <th>Status</th>
      <th>Action</th>
    </tr>
  </thead>
  <tbody>
    <%= render partial: "admin/users/user", collection: @users%>
  </tbody>
</table>
<%= will_paginate @users, renderer: BootstrapPagination::Rails%>